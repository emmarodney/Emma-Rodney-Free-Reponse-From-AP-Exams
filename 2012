4) (a) 
public int countWhitePixels() {
	int count = 0;
	for( int i = 0; i < pixelValues; i++)
		for(int j = 0; j < pixelValues; j++)
			if(pixelValues = “white” ) 
				count++ ; 
}

(b) 
public void processImage() {
	for (int i = 0; i < pixelValues; i++)
		for (int j = 0; j < pixelValues; j++)
			pixelValues = pixelValues – 2; // don't really know what im doing for that one 
}


